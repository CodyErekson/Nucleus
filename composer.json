{
    "name": "chillem/nucleus",
    "description": "PHP application scaffold",
    "keywords": ["PHP", "Slim", "API", "framework", "Twig", "Bootstrap"],
    "license": "MIT",
    "authors": [
        {
            "name": "Cody Erekson",
            "email": "cody.erekson@gmail.com",
            "homepage": "http://codyerekson.me/"
        }
    ],
    "require": {
        "php": "^7.2",
        "slim/slim": "3.*@dev",
        "monolog/monolog": "^1.17",
        "vlucas/phpdotenv": "^2.4.0",
        "illuminate/database": "^5.5",
        "robmorgan/phinx": "^0.9.2",
        "firebase/php-jwt": "^5.0",
        "tuupola/slim-jwt-auth": "^2.3",
        "ramsey/uuid": "^3.7",
        "dopesong/slim-whoops": "^2.2",
        "slim/twig-view": "^2.3",
        "chillem/dbug": "dev-master",
        "respect/validation": "^1.1",
        "slim/csrf": "^0.8.2",
        "slim/flash": "^0.4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5"
    },
    "autoload": {
        "psr-4": {
            "Nucleus\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NucleusTest\\": "tests/phpunit/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "version": "1.0.0"
}
