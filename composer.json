{
    "name": "chillem/dope",
    "description": "Dope Game Core",
    "keywords": ["PHP", "Slim", "API", "framework", "Twig", "Bootstrap"],
    "license": "MIT",
    "authors": [
        {
            "name": "Cody Erekson",
            "email": "cody.erekson@gmail.com",
            "homepage": "http://codyerekson.me/"
        }
    ],
    "require": {
        "php": "^7.2",
        "slim/slim": "3.*@dev",
        "monolog/monolog": "^1.17",
        "vlucas/phpdotenv": "^2.4.0",
        "illuminate/database": "^5.5",
        "robmorgan/phinx": "^0.10.6",
        "firebase/php-jwt": "^5.0",
        "tuupola/slim-jwt-auth": "^3.1",
        "ramsey/uuid": "^3.7",
        "slim/twig-view": "^2.3",
        "chillem/dbug": "dev-master",
        "respect/validation": "^1.1",
        "slim/csrf": "^0.8.2",
        "slim/flash": "^0.4.0",
        "league/event": "^2.1",
        "league/climate": "^3.2",
        "adrianfalleiro/slim-cli-runner": "^2.4",
        "akrabat/ip-address-middleware": "^1.0",
        "guzzlehttp/guzzle": "^6.3",
        "swiftmailer/swiftmailer": "^6.0",
        "pelago/emogrifier": "^2.0",
        "symfony/yaml": "^4.1",
        "gumlet/php-image-resize": "1.9.*"
    },
    "require-dev": {
        "dopesong/slim-whoops": "^2.2",
        "phpunit/phpunit": "^6.5",
        "squizlabs/php_codesniffer": "*"
    },
    "autoload": {
        "psr-4": {
            "Nucleus\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NucleusTest\\": "tests/phpunit/"
        }
    },
    "scripts": {
        "cscheck": "php vendor/bin/phpcs --standard=PSR1,PSR2 src/",
        "csfix": "php vendor/bin/phpcbf --standard=PSR1,PSR2 src/",
        "cs": [
            "@cscheck",
            "@csfix"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "version": "0.1.0"
}
